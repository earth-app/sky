{
  "private": true,
  "name": "sky",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "generate": "nuxt build",
    "generate:ios": "nuxt build --dotenv .env.ios",
    "generate:android": "nuxt build --dotenv .env.android",
    "build:ios": "bunx cap sync && bunx cap build ios",
    "build:android": "bunx cap sync && bunx cap build android",
    "dev": "bunx nuxi dev --dotenv .config/local.env --no-restart --public --port 3001",
    "dev:remote": "bunx nuxi dev --dotenv .config/production.env --dotenv .env --no-restart --public --port 3001",
    "dev:android": "bash scripts/dev-android.sh",
    "dev:ios": "bash scripts/dev-ios.sh",
    "sync": "bunx cap sync",
    "sync:assets": "bun run capacitor-assets generate --ios --android",
    "prettier": "bunx prettier --write .",
    "prettier:check": "bunx prettier --check .",
    "postinstall": "nuxt prepare",
    "prepare": "husky install"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/core": "^8.0.2",
    "@capacitor/dialog": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.2",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@capacitor/toast": "^8.0.0",
    "@earth-app/crust": "0.2.0",
    "@earth-app/ocean": "1.0.1",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/vue": "^8.7.17",
    "@nuxt/icon": "^2.2.1",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.4.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@nuxtjs/ionic": "^1.0.2",
    "@tiptap/extension-drag-handle": "^3.18.0",
    "@types/luxon": "^3.7.1",
    "luxon": "^3.7.2",
    "nuxt": "^4.3.0",
    "nuxt-viewport": "^2.3.1",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@capacitor/cli": "8.0.1",
    "@iconify-json/logos": "^1.2.10",
    "@ionic/cli": "^7.2.1",
    "@prettier/plugin-xml": "^3.4.2",
    "@vue/cli-service": "5.0.9",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "vue-tsc": "^3.2.4",
    "wrangler": "^4.61.0"
  },
  "lint-staged": {
    "*.{ts,vue,css,md,json,xml,plist,storyboard,yml}": "prettier --write"
  }
}
